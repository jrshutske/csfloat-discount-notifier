{
  "name": "csfloat-price-check",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "start": "node --require=suppress-experimental-warnings --loader ts-node/esm --experimental-specifier-resolution=node src/app.ts",
    "bundle": "esbuild src/app.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist-bundle/app.js --external:fsevents --define:import.meta.url='\"file://\"+__filename'",
    "build:exe": "pnpm bundle && pkg dist-bundle/app.js --targets node18-win-x64 --output build/csfloat-notifier-win.exe && node scripts/copy-assets.cjs",
    "build:exe:macos": "pnpm bundle && pkg dist-bundle/app.js --targets node18-macos-x64,node18-macos-arm64 --output build/csfloat-notifier-macos && node scripts/copy-assets.cjs",
    "build:exe:linux": "pnpm bundle && pkg dist-bundle/app.js --targets node18-linux-x64 --output build/csfloat-notifier-linux && node scripts/copy-assets.cjs",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "author": "",
  "license": "ISC",
  "pkg": {
    "assets": [
      "icon.png"
    ]
  },
  "dependencies": {
    "@types/openurl": "^1.0.2",
    "axios": "^1.5.1",
    "chalk": "4.1.2",
    "cli-spinners": "^2.9.1",
    "commander": "^11.1.0",
    "dotenv": "^17.2.1",
    "node-notifier": "9.0.0",
    "open": "^9.1.0",
    "openurl": "^1.1.1",
    "ora": "5.4.1",
    "suppress-experimental-warnings": "^1.1.17",
    "terminal-link": "^3.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@types/node": "^20.8.8",
    "@types/node-notifier": "^8.0.4",
    "@yao-pkg/pkg": "^6.9.0",
    "barrelsby": "^2.8.1",
    "esbuild": "^0.25.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.4"
}
